{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "lib": ["ES2018", "DOM"],
    "esModuleInterop": true,
    "declaration": false,
    "noEmitOnError": false,
    "alwaysStrict": false,
    "noImplicitUseStrict": true
  },
  "include": ["extension/**/*.ts", "utils/*.ts", "test/**/*.ts"]
}
